        "lower= 1042000\n",
        "upper= 702648265\n",
        "\n",
        "for num in range(lower , upper+1):\n",
        "\n",
        "  order = len(str(num))\n",
        "  sum=0\n",
        "  temp=num\n",
        "  while temp > 0:\n",
        "        digit=temp%10\n",
        "        sum +=digit**order\n",
        "        temp //=10\n",
        "        \n",
        "        \n",
        "  if  num==sum:\n",
        "         print(num)\n"
